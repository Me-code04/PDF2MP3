{
  "name": "theme",
  "version": "1.0.0",
  "description": "Tailwind + DaisyUI setup for PDF2MP3 Django app",
  "type": "commonjs",
  "main": "postcss.config.js",
  "scripts": {
    "build": "node_modules/.bin/postcss ./src/styles.css -o ../static/css/dist/styles.css",
    "watch": "node_modules/.bin/postcss ./src/styles.css -o ../static/css/dist/styles.css --watch",
    "dev": "cross-env NODE_ENV=development node_modules/.bin/postcss ./src/styles.css -o ../static/css/dist/styles.css --watch",
    "build:prod": "cross-env NODE_ENV=production node_modules/.bin/postcss ./src/styles.css -o ../static/css/dist/styles.css --minify"
  },
  "devDependencies": {
    "autoprefixer": "10.4.14",
    "cross-env": "^7.0.3",
    "daisyui": "4.12.10",
    "postcss": "8.4.24",
    "postcss-cli": "10.1.0",
    "tailwindcss": "3.4.1"
  },
  "license": "MIT"
}
